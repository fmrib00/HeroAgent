# Azure Table Storage Configuration
connection_string=DefaultEndpointsProtocol=https;AccountName=<your-account-name>;AccountKey=<your-account-key>;EndpointSuffix=core.windows.net

# API Configuration
API_KEY=<generate-a-random-api-key>
API_PORT=8081
API_ENV=development
SWAGGER_ENABLED=true

# JWT Secret Key (generate a strong random string)
JWT_SECRET_KEY=<your-jwt-secret-key>

# SMTP Configuration for Registration Emails
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=<your-smtp-username>
SMTP_PASSWORD=<your-smtp-password>
EMAIL_FROM=<your-from-email>

# CORS Configuration
ALLOWED_ORIGINS=*
